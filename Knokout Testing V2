local Interval
Tab:AddTextbox({
    Name = "Interval",
    Default = "0.1",
    TextDisappear = false,
    Callback = function(Value)
        Interval = Value
        if Value == "0.01" or Value == "0.02" or Value == "0.03" or Value == "0.04" or Value == "0.05" or Value == "0.06" or Value == "0.07" or Value == "0.08" or Value == "0.09" then
    OrionLib:MakeNotification({
	Name = "        INTERVAL WARNING",
	Content = "Some low devices won't handle low intervals! Make sure to use 0.1 or more if you have a device that doesn't handle fast spamming.",
	Image = "rbxassetid://0",
	Time = 15
       })
        end
    end
})

Tab:AddButton({
	Name = "Turn On Autofarm",
	Callback = function()
    OrionLib:MakeNotification({
	Name = "AUTOFARM",
	Content = "Autofarm is on, unequip to stop.",
	Image = "rbxassetid://4483345998",
	Time = 5
})
       local player = game.Players.LocalPlayer
            for i = 0, math.huge, 1 do --se repetera 10 fois
 player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
  player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
   player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
    player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
     player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
      player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
       player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
       player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
  player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
   player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
    player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
     player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
      player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
       player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
       player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
  player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
   player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
    player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
     player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
      player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
       player.Character:FindFirstChildWhichIsA("Tool").ExpRemote:FireServer()
    task.wait(Interval)
end
end
})
local WalkSpeed
local JumpPower
Tab2:AddToggle({
	Name = "Sit",
	Default = false,
	Callback = function(Value)
		if Value == true then
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
        elseif Value == false then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
        end    
        end
})

Tab2:AddToggle({
	Name = "Platform Stand",
	Default = false,
	Callback = function(Value)
		if Value == true then
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
        elseif Value == false then
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
        end    
        end
})

Tab2:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(125,125,125),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
    WalkSpeed = Value
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
         Value = WalkSpeed
	end    
})

Tab4:AddTextbox({
    Name = "Custom Music (ID Required)",
    Default = "1837362419",
    TextDisappear = false,
    Callback = function(Value)
        workspace.Music.SoundId = "rbxassetid://"..Value
    end
})

Tab4:AddButton({
	Name = "Reset Music",
	Callback = function()
    OrionLib:MakeNotification({
	Name = "                   MUSIC",
